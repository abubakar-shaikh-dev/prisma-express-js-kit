{
    "name": "prisma-express-js-kit",
    "version": "1.0.0",
    "main": "src/server.js",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production && npm run prisma:push && node src/server.js",
        "dev": "cross-env NODE_ENV=development nodemon src/server.js",
        "format:check": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "npm run test",
        "prepare": "husky",
        "prisma:generate": "cd src && npx prisma generate",
        "prisma:migrate": "cd src && npx prisma migrate",
        "prisma:pull": "cd src && npx prisma db pull",
        "prisma:push": "cd src && npx prisma db push",
        "prisma:reset": "cd src && npx prisma db reset",
        "prisma:seed": "cd src && npx prisma db seed",
        "prisma:studio": "cd src && npx prisma studio"
    },
    "author": "Abubakar Shaikh <shaikhabubakar2380@gmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abubakar-shaikh-dev/prisma-express-js-kit.git"
    },
    "bugs": {
